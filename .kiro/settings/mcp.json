{
  "mcpServers": {
    "pulsehub-api-autenticacao": {
      "command": "node",
      "args": ["./servidores-mcp/api-autenticacao.js"],
      "env": {
        "NODE_ENV": "development",
        "JWT_SECRET": "sua-chave-secreta-jwt",
        "JWT_EXPIRES_IN": "24h"
      },
      "disabled": false,
      "autoApprove": ["autenticar", "registrar", "redefinirSenha"]
    }
  }
}